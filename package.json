{
	"name": "fuse-snippets",
	"displayName": "fuse-snippets",
	"description": "An snippet extension to help create Fuses for Fusion and Davinci Resolve",
	"version": "0.2.0",
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Snippets",
		"Other"
	],
	"publisher": "rne1223",
	"author": {
		"email": "rne1223@gmail.com",
		"name": "Rene Tellez Rodriguez"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/rne1223/fuse-snippets"
	},
	"icon": "images/DaFusion_logo.png",
	"keywords": [
		"Fuse",
		"Davinic Resolve",
		"Fusion",
		"fuse snippets"
	],
	"activationEvents": [
		"onLanguage:fuse",
		"onCommand:fuse-snippets.CreateVars"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "fuse",
				"extensions": [
					".fuse"
				],
				"aliases": [
					"Fuse",
					"fuse"
				]
			}
		],
		"snippets": [
			{
				"language": "lua",
				"path": "./snippets/fuse.code-snippets"
			}
		],
		"commands": {
			"title": "Create Vars in Process()",
			"category": "Fuse-snippets",
			"command": "fuse-snippets.CreateVars"
		}
	},
	"extensionDependencies": [
		"sumneko.lua"
	],
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.54.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}
